{
  "usePublicUrl":true,
  "defaultConfig":{
    "lang": "zh"
  },
  "redirect":[
    {
      "matchMethod": ["*"],
      "matchUrl": {
        "pathname": "{/}assets/*"
      },
      "to": {
        "url": "{{pattern.pathname.input}}",
        "appendHeaders": {},
        "removeHeaders": []
      }
    },
    {
      "matchMethod":["*","GET"],
      "matchUrl":{
        "pathname":"*"
      },
      "to": {
        "url": "/{{lang}}{{pattern.pathname.input}}",
        "appendHeaders":{
        },
        "removeHeaders":[".*"]
      }
    }
  ]
}